FAST_RAM_START := 0xE30800
FAST_RAM_END := 0xE30800+1024;E30C00


;STACK_START := FAST_RAM_START
;STACK_END :=	STACK_START+0x3F/2

VARIABLE_MEMORY_START := FAST_RAM_START
VARIABLE_MEMORY_END := VARIABLE_MEMORY_START+0x7F-16

DIV_16 := VARIABLE_MEMORY_END+1 
DIV_16_END := DIV_16+142

OCTANT_FAST_RAM_START := DIV_16_END+1
OCTANT_FAST_RAM_END := OCTANT_FAST_RAM_START+170

SCALE_DOWN_FAST_RAM_START := OCTANT_FAST_RAM_END+1
SCALE_DOWN_FAST_RAM_END := SCALE_DOWN_FAST_RAM_START+-1

SCALE_UP_FAST_RAM_START := SCALE_DOWN_FAST_RAM_END+1
SCALE_UP_FAST_RAM_END := SCALE_UP_FAST_RAM_START+SCALE_UP_SIZE



SCALE_UP_CLIPPED_FAST_RAM_START := SCALE_UP_FAST_RAM_END+1
SCALE_UP_CLIPPED_FAST_RAM_END := SCALE_UP_CLIPPED_FAST_RAM_START+SCALE_UP_CLIPPED_SIZE


SCALE_UP_LOOP_FAST_RAM_START := SCALE_UP_CLIPPED_FAST_RAM_END+1
SCALE_UP_LOOP_FAST_RAM_END := SCALE_UP_LOOP_FAST_RAM_START+SCALE_UP_LOOP_SIZE


if SCALE_UP_LOOP_FAST_RAM_END > FAST_RAM_END
        err 'Fastram size exceeded'
end if
